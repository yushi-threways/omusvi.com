{% extends 'base.html.twig' %}

{% block title %}MyEventApplication index{% endblock %}

{% block body %}
    <h1>MyEventApplication index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Status</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for my_event_application in my_event_applications %}
            <tr>
                <td>{{ my_event_application.id }}</td>
                <td>{{ my_event_application.status }}</td>
                <td>{{ my_event_application.createdAt ? my_event_application.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ my_event_application.updatedAt ? my_event_application.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('my_event_application_show', {'id': my_event_application.id}) }}">show</a>
                    <a href="{{ path('my_event_application_edit', {'id': my_event_application.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('my_event_application_new') }}">Create new</a>
{% endblock %}
